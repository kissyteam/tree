<div class="{{getBaseCssClasses ('row')}}
     {{#if (selected)}}
        {{getBaseCssClasses ('selected')}}
     {{/if}}
     ">
    <div class="{{getBaseCssClasses ('expand-icon')}}">
    </div>
    {{#if (checkable)}}
    <div class="{{getBaseCssClasses ('checked'+checkState)}} {{getBaseCssClasses ('checked')}}"></div>
    {{/if}}
    <div class="{{getBaseCssClasses ('icon')}}">

    </div>
    <div class="{{getBaseCssClasses ('content')}}">{{{content}}}</div>
</div>
<div class="{{getBaseCssClasses ('children')}}"
{{#if (!expanded)}}
style="display:none"
{{/if}}
>
</div>